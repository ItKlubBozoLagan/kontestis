{
  "compilerOptions": {
    "composite": true,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "lib": ["ESNext"],
    "target": "ES2022",
    "downlevelIteration": true,
    "moduleResolution": "node",
    "strict": true,
    "baseUrl": ".",
    "paths": {
      "@/*": ["./src/*"]
    }
  },
  "exclude": ["node_modules"],
  "include": ["vite.config.ts"]
}
